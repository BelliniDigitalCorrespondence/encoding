	<text n="LL1.31.II" subtype="manuscript" type="minuta_di_lettera" xml:id="LL1.31.II">
		<front />

		<body xml:lang="fr">
			<pb facs="#LL1.31.II_folio_1r" n="1" xml:id="LL1.31.II_fol_1r" />
			<div n="t_01" type="letter-body">
				<ab n="ab_01" rend="all_indented" type="parag">
					<s n="s_01" type="sentence">
						<ptr target="#LL1.31.II_note1" />
						<lb facs="#LL1.31.II_line_1r_01" n="1" xml:id="LL1.31.II_lb_1r_01" />
						J'aurais besoin de savoir
						<lb facs="#LL1.31.II_line_1r_02" n="2" xml:id="LL1.31.II_lb_1r_02" />
						le nom de la personne qui
						<lb facs="#LL1.31.II_line_1r_03" n="3" xml:id="LL1.31.II_lb_1r_03" />
						vous a demandé la
						<term ref="TEI-ListTerm.xml#partitura" type="music-term">partition</term>
						<lb facs="#LL1.31.II_line_1r_04" n="4" xml:id="LL1.31.II_lb_1r_04" />
						des
						<rs ref="TEI-ListWork.xml#Puritani" rend="underline" type="work">Puritains</rs>
						pour le
						<lb facs="#LL1.31.II_line_1r_05" n="5" xml:id="LL1.31.II_lb_1r_05" />
						<orgName ref="TEI-ListOrganization.xml#Semperoper" type="theatre">Theatre de Dresde</orgName>
					</s>
				</ab>
			</div>
			<div type="paratext">
				<ab facs="#LL1.31.II_hotspot_1r_01" hand="#h1" subtype="addition" type="paratext" xml:id="LL1.31.II_hs_1r_01">
