							<lb facs="#LL1.2_line_2v_01" n="1" rend="align(right)" xml:id="LL1.2_lb_2v_01_front" />
							<addrLine>Madame</addrLine>
							<lb facs="#LL1.2_line_2v_02" n="2" rend="align(center)" xml:id="LL1.2_lb_2v_02_front" />
							<addrLine>Madame <persName ref="TEI-ListPerson.xml#CantuTurinaGiuditta">Judith Turina</persName>
							</addrLine>
							<lb facs="#LL1.2_line_2v_03" n="3" rend="align(center)" xml:id="LL1.2_lb_2v_03_front" />
							<addrLine>
								<placeName ref="TEI-ListPlace.xml#CE">Cremone</placeName> pour
								<placeName ref="TEI-ListPlace.xml#Casal" type="writing"><hi rend="underline">Casalbuttano</hi></placeName>
