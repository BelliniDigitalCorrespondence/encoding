	<text n="LL1.34.I" subtype="manuscript" type="minuta_di_lettera" xml:id="LL1.34.I">
		<front />

		<body xml:lang="it">
			<pb facs="#LL1.34.I_folio_1r" n="1" xml:id="LL1.34.I_fol_1r" />
			<!-- recto -->
			<div n="t_01" type="letter-body">
				<salute n="ab_01">
					<s n="s_01"  type="sentence">
						<ptr target="#LL1.34.I_note1" />
						<lb facs="#LL1.34.I_line_1r_01" n="1" xml:id="LL1.34.I_lb_1r_01" />
						<hi rend="align(center)">
							<rs ref="TEI-ListPerson.xml#DelCarrettoFrancescoSaverio" type="person">Eccellenza</rs>
						</hi>
					</s>
				</salute>
				<space quantity="4" unit="line" /> <!-- prossemica -->
				<ab n="ab_02" type="parag">
					<s n="s_02" type="sentence">
						<lb facs="#LL1.34.I_line_1r_02" n="2" xml:id="LL1.34.I_lb_1r_02" />
						La bontà con la quale si degnò<w>
							ricever
							<lb break="no" facs="#LL1.34.I_line_1r_03" n="3" xml:id="LL1.34.I_lb_1r_03" />
							mi</w>
						l'ultima volta che io fui in
						<placeName ref="TEI-ListPlace.xml#NA">Napoli</placeName>
						<lb facs="#LL1.34.I_line_1r_04" n="4" xml:id="LL1.34.I_lb_1r_04" />
						mi dà <w resp="#LM">il</w> coragio di pregarla acciò si voglia
						<lb facs="#LL1.34.I_line_1r_05" n="5" xml:id="LL1.34.I_lb_1r_05" />
						interessarsi in un'affare che mi riguarda.
					</s>
					<s n="s_03" type="sentence">
						<lb facs="#LL1.34.I_line_1r_06" n="6" xml:id="LL1.34.I_lb_1r_06" />
						In Italia si fà una
						<hi rend="underline">Pirateria</hi><space/>di tutto ciò che
						<lb facs="#LL1.34.I_line_1r_07" n="7" xml:id="LL1.34.I_lb_1r_07" />
						dovrebbe essere più rispettato e
						<subst>
							<del>
								<gap reason="deleted" extent="one-word" />
							</del>
							<add place="above">sostenuto</add>
						</subst>
						dalle
						<lb facs="#LL1.34.I_line_1r_08" n="8" xml:id="LL1.34.I_lb_1r_08" />
						leggi come lo è in Francia ed in Inghilterra
						<lb facs="#LL1.34.I_line_1r_09" n="9" xml:id="LL1.34.I_lb_1r_09" />
						voglio dire,
						<hi rend="underline">della proprietà d'Autore</hi>.
					</s>
					<s n="s_04" type="sentence">
						Come
						<lb facs="#LL1.34.I_line_1r_10" n="10" xml:id="LL1.34.I_lb_1r_10" />
						<rs ref="TEI-ListPerson.xml#DelCarrettoFrancescoSaverio" resp="#GS" type="person">Ella</rs>
						sà, ho scritto per
						<placeName ref="TEI-ListPlace.xml#Paris">Parigi</placeName>
						<rs ref="TEI-ListWork.xml#Puritani" type="work">
							i
							<hi rend="underline italic">Puritani</hi>:
						</rs>
						<del rend="strikethrough">questi si</del>
						<lb facs="#LL1.34.I_line_1r_11" n="11" xml:id="LL1.34.I_lb_1r_11" />
						<del rend="strikethrough">sono st</del>
						di quest'<term ref="TEI-ListTerm.xml#opera" subtype="music-term">opera</term>
						si son stampati,
						<hi rend="underline">per solo</hi>
						<lb facs="#LL1.34.I_line_1r_12" n="12" xml:id="LL1.34.I_lb_1r_12" />
						<hi rend="underline">canto</hi><space/>diversi pezzi; ma la grande
						<term ref="TEI-ListTerm.xml#partitura" subtype="music-term">partizione</term>
						<lb facs="#LL1.34.I_line_1r_13" n="13" xml:id="LL1.34.I_lb_1r_13" />
						con lo strumentale è restata inedita; quindi
						<lb facs="#LL1.34.I_line_1r_14" n="14" xml:id="LL1.34.I_lb_1r_14" />
						di mia proprietà: ora con mia meraviglia sento
						<lb facs="#LL1.34.I_line_1r_15" n="15" xml:id="LL1.34.I_lb_1r_15" />
						che la
						<orgName ref="TEI-ListOrganization.xml#Soc_teatri_NA" resp="#EC" type="opera-org">Società dei teatri di Napoli</orgName>
						andrà
						<lb facs="#LL1.34.I_line_1r_16" n="16" xml:id="LL1.34.I_lb_1r_16" />
						a dare
						<rs ref="TEI-ListWork.xml#Puritani" type="work">
							<hi rend="underline">i Puritani</hi></rs>
						<del rend="dotted"><gap reason="deleted" unit="char" quantity="1"/></del>
						senza averla da me
						<lb facs="#LL1.34.I_line_1r_17" n="17" xml:id="LL1.34.I_lb_1r_17" />
						avuto.
					</s>
					<s n="s_05" type="sentence">
						<subst>
							<del rend="strikethrough">
								I feudi d'<choice resp="#DS #LM"><orig>un'</orig><reg>un</reg></choice>autore,
								<rs ref="TEI-ListPerson.xml#DelCarrettoFrancescoSaverio" type="person">Eccellenza,</rs>
							</del>
							<add place="supralinear">La proprietà di terra</add>
						</subst>
						non
						<lb facs="#LL1.34.I_line_1r_18" n="18" xml:id="LL1.34.I_lb_1r_18" />
						sono che il parto del suo ingegno:
						<subst>
							<del instant="true" rend="strikethrough">e se la</del>
							<lb facs="#LL1.34.I_line_1r_19" n="19" xml:id="LL1.34.I_lb_1r_19" />
							<del instant="true" rend="strikethrough">
							<add place="margin">
								<gap reason="deleted" extent="one-word"/>
							</add>
							<add place="supralinear"> d'un</add>
							terra</del>
							<add place="inline"> e se la proprietà</add>
						</subst>
						dei primi è sacra
						<lb facs="#LL1.34.I_line_1r_20" n="20" xml:id="LL1.34.I_lb_1r_20" />
						quella del secondo lo debba essere del pari.
					</s>
					<s n="s_06" type="sentence">
						<lb facs="#LL1.34.I_line_1r_21" n="21" xml:id="LL1.34.I_lb_1r_21" />
						perciò io mi rivolgo alla severa
						<subst>
							<add place="supralinear">
								giustizia di
								<choice>
									<abbr><rs ref="TEI-ListPerson.xml#DelCarrettoFrancescoSaverio">V<am>.</am><space/>E<am>.</am></rs></abbr>
									<expan>V<ex>ostra</ex><space/>E<ex>ccellenza</ex></expan>
								</choice>
							</add>
							<del rend="strikethrough">e giusto</del>
							<lb facs="#LL1.34.I_line_1r_22" n="22" xml:id="LL1.34.I_lb_1r_22" />
							<del rend="strikethrough">raziocinio di
								<rs ref="TEI-ListPerson.xml#DelCarrettoFrancescoSaverio" type="person">
									<choice>
										<abbr>V<am>.</am><space/>E<am>.</am></abbr>
										<expan>V<ex>ostra</ex><space/>E<ex>ccellenza</ex></expan>
									</choice>
								</rs>
								che mi sembrò scorgerlo
							</del>
							<lb facs="#LL1.34.I_line_1r_23" n="23" xml:id="LL1.34.I_lb_1r_23" />
							<del rend="strikethrough">in quanti discorsi ebbi io l'onore d'ascoltare delle
							</del>
